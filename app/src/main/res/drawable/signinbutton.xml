<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="50dp"
        />
    <gradient
        android:angle="45"
        android:centerX="44%"
        android:centerColor="@color/colorSnapcaht"
        android:startColor="@color/colorSnapcaht"
        android:endColor="@color/colorSnapcaht"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="160dp"
        android:height="60dp"
        />
    <stroke
        android:width="3dp"
        android:color="@color/colorSnapcaht"
        />
</shape>